#include<iostream>
using namespace std;
#include<vector>
int main() {
    vector<int> arr = {3, 5, 10};
    arr.push_back(8);
    arr.push_back(2);
    arr.pop_back( );
    for(int val : arr) {
        cout<<val<<" ";
    }
}


OUTPUT :
3 5 10 8 

=== Code Execution Successful ===

AND FIND THE SIZE OF CODE
#include<iostream>
using namespace std;
#include<vector>
int main() {
    vector<int> arr = {3, 5, 10};
    arr.push_back(8);
    arr.push_back(2);
    arr.pop_back( );
    int n = arr.size();
    cout<<n<<endl;
    for(int val : arr) {
        cout<<val<<" ";
    }
}

OUTPUT:

4
3 5 10 8 

=== Code Execution Successful
